    <Setting Name="sqlSelectMouvements" Type="System.String" Scope="Application">
      <Value Profile="(Default)">select Id , catégorie, sousCatégorie, montant , sens , tiers , note , dateMvt , dateCréation , dateModification , etat , événement , type , modifiable , numeroRemise , idCheque from Mouvements;</Value>
