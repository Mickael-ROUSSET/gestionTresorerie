    <Setting Name="sqlSelectMouvementsLibelles" Type="System.String" Scope="Application">
      <Value Profile="(Default)">select Id , (select libelle from Categorie where id = catégorie) as Catégorie, (select libelle from sousCategorie where id = sousCatégorie) as sousCatégorie, montant , sens , (select coalesce(nom + ' ' + prenom, raisonSociale ) from Tiers where id = tiers) as Tiers, note , dateMvt , dateCréation , dateModification , etat , événement , type , modifiable , numeroRemise , idCheque from Mouvements;</Value>
